//PROTOPIE
void checkProtoPieSerial(const char* idCercato) {
    if (Serial.available() > 0) { 
        Serial.setTimeout(50); 
        messaggio = Serial.readStringUntil('\0');//versiona compatta
        int delimiterIndex = messaggio.indexOf("||");
        if (delimiterIndex != -1) {
            if (messaggio.startsWith(idCercato)) {
                messaggio.remove(0, strlen(idCercato) + 2);
            } 
            else {
                messaggio = "";//pulizia
            }
        } else {
            messaggio = "";//ulizia
        }
        while(Serial.available() > 0) { 
					Serial.read(); 
				}
    }
}


//COMODITA MACCHINA STATI
void returnToIdle () {
			currentState = STATE_IDLE;
