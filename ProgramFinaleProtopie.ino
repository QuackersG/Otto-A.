//FUNZIONE DI SCRITTURA, spaziata
void write(const String &text, int stepSize) {
  u8g2.setFont(u8g2_font_minuteconsole_tr);
  int totalLength = text.length();
  int16_t lineHeight = u8g2.getAscent() - u8g2.getDescent();

  for (int charLimit = 0; charLimit <= totalLength; charLimit += stepSize) {
    
    u8g2.firstPage();
    do {
      int16_t currentX = 0;
      int16_t currentY = u8g2.getAscent();
      int charsPrinted = 0;
      const char* p = text.c_str();

      while (*p != '\0' && currentY < u8g2.getDisplayHeight() && charsPrinted < charLimit) {
        const char* end = p;
        while (*end != ' ' && *end != '\0') end++;
        int wordLen = end - p;

        char wordBuffer[21];
        if (wordLen > 20) wordLen = 20;
        strncpy(wordBuffer, p, wordLen);
        wordBuffer[wordLen] = '\0';

        int16_t wordWidth = u8g2.getStrWidth(wordBuffer);

        if (currentX + wordWidth > u8g2.getDisplayWidth() && currentX != 0) {
          currentX = 0;
          currentY += lineHeight;
        }

        if (currentY < u8g2.getDisplayHeight()) {
          u8g2.setCursor(currentX, currentY);
          for (int i = 0; i < wordLen; i++) {
            if (charsPrinted < charLimit) {
              u8g2.print(wordBuffer[i]);
              charsPrinted++;
            }
          }
          currentX += wordWidth + u8g2.getStrWidth(" ");
          charsPrinted++;
        }
        
        p = end;
        if (*p == ' ') p++;
      }
    } while (u8g2.nextPage());

    //PAUSE VARIABILI
    int pausaDinamica = 10; 

    if (charLimit > 0 && charLimit <= totalLength) {
      char lastChar = text[charLimit - 1];

      if (lastChar == ' ') {
        pausaDinamica = 300; 
      } 
      else if (lastChar == '.' || lastChar == '!' || lastChar == '?') {
        pausaDinamica = 500; 
      }
      else if (lastChar == ',') {
        pausaDinamica = 250; 
      }
			
			if ((lastChar != ' ')&&(lastChar != ' ,')&&(lastChar != '.')&&(lastChar != '?')&&(lastChar != '!')) {
        int frequency = 400 + (lastChar * 10);
        tone(PIEZP, frequency, 20);
      }
    }

    delay(pausaDinamica); 
  }
}

//FUNZIONI <3
void detectCaress () {
  unsigned int PR1 = analogRead(PR1P);
  unsigned int PR2 = analogRead(PR2P);
  if ((PR1 > CaressTR) && (!CaressMaybe)) {
    CaressS = millis();
    CaressMaybe = true;
  }
  if ((CaressMaybe) && (PR1 < CaressTR) && (PR2 > CaressTR)) {
    CaressMaybe = false;
    Serial.println(F("action||1"));
		feedbackType = 1;
  }
  if ((CaressMaybe) && (millis() - CaressS > 500)) {
    CaressMaybe = false;
  }
}

//FUNZIONI :)
void detectShake() {
  bool isCurrentlyAbove = (totalMovementAccel > SHAKE_PEAK_THRESHOLD);

  if (isCurrentlyAbove && !wasAboveThreshold_shake && (millis() - tN_shake > 300)) {
    dV_shake = dN_shake;
    tV_shake = tN_shake;
    tN_shake = millis();

    if (tV_shake > 0) {
      dN_shake = tN_shake - tV_shake;
    }

    if (dV_shake > 0 && dN_shake > 0 && (dN_shake + dV_shake < SHAKE_WINDOW_MS)) {
      Serial.println(F("action||2"));
      tN_shake = 0; tV_shake = 0; dN_shake = 0; dV_shake = 0;
			feedbackType = 2;
    }
  }
  
  wasAboveThreshold_shake = isCurrentlyAbove;
}


//FUNZIONI :(
void detectLayDownState() {
  bool isLayingDownAndStill = (abs(accelZ) < LAY_DOWN_Z_THRESHOLD &&
                               totalMovementAccel < STILL_MOVEMENT_THRESHOLD &&
                               abs(gyroX) < STILL_GYRO_THRESHOLD &&
                               abs(gyroY) < STILL_GYRO_THRESHOLD);

  if (isLayingDownAndStill) {
    if (!isPotentiallySad) {
      isPotentiallySad = true;
      sadStateStartTime = millis();
    }
  } 
  else {
    if (isPotentiallySad) {
      isPotentiallySad = false;
    }
  }

  if (isPotentiallySad && (millis() - sadStateStartTime > SAD_STATE_DURATION_MS)) {
    Serial.println(F("action||3"));
    isPotentiallySad = false;
		feedbackType = 3;
  }
}

//FUNZIONE DI DETECT FEEDBACK COMPLETA
void detectFeedback() {
    readSensors();
    detectShake();
    detectLayDownState();
    detectCaress();
    switch (SlamState) {
      case WAIT_STILL:
        
        if (totalMovementAccel < STILL_MOVEMENT_THRESHOLD) {
          if (millis()-startStill > 700) {
            SlamState = STILL;
          } 
        } else {
          startStill = millis();
        }
        break;

      case STILL:
        if (totalMovementAccel > STILL_MOVEMENT_THRESHOLD) {
          SlamState = LIFT;
          startLift = millis();
          liftf = false;
        }
        break;

      case LIFT:
        if (accelZ+1 < 0) { 
          if (liftf) {
            SlamState = DROP;
            startDrop = millis();
            dropf = false;
          } else {
            SlamState = WAIT_STILL;
          }
        }
        if (millis() - startLift > 500) {
          
            //Serial.println("tempo scaduto");
          SlamState = WAIT_STILL;
        }
        if (abs(accelZ+1) > LIFT_THRESHOLD) {
          //Serial.println("Sono abbastanza veloce!");
          liftf = true;
        }
        break;

        case DROP:
          if (accelZ-1 > 0) {
            //Serial.println("Mi sto muovendo verso il basso!");
            if (dropf) {
              //Serial.println("Dropping terminato!");
              SlamState = STILL_II;
              startStill_II = millis();
            } else {
              SlamState = WAIT_STILL;
            }
          }
          if (millis() - startDrop > 300) {
            SlamState = WAIT_STILL;
          }
          if (abs(accelZ-1) > DROP_THRESHOLD-0.5) {
            //Serial.println("Sono abbastanza veloce! basso");
            dropf = true;
          }
          break;

        case STILL_II:
          //Serial.println("Still_II-Bot!");
            if (millis()-startStill_II > 500) {
              Serial.println(F("action||4"));
              SlamState = WAIT_STILL;
							feedbackType = 4;
            } else if (totalMovementAccel > STILL_MOVEMENT_THRESHOLD+0.5){
              SlamState = WAIT_STILL;
            }
        break;
    }
}



