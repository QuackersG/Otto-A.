//--------------------------------SETUP-------------------------------

void setup() {
  Serial.begin(9600);
  while (!Serial);

	//PIEZO
	pinMode(PIEZP, OUTPUT);

	//SETUP PRESSIONE
	pinMode(PR1P,INPUT);
	pinMode(PR2P,INPUT);

  //SETUP IMU
  Wire.begin();
  mpu6050.begin();

  Serial.println(F("Calibrazione giroscopio..."));
  mpu6050.calcGyroOffsets(true);
  Serial.println(F("Calibrazione completata!"));

  //SETUP SCHERMO
  messaggio.reserve(110); 
  u8g2.begin();
	u8g2.sendF("c", 0xa6);
  u8g2.setContrast(200);
}





//--------------------------------LOOP-------------------------------

void loop() {
	
  switch (currentState) {
		
    case STATE_IDLE:

			if (millis() - FaceAuxTime > 300) {

				if (expr == 0) {
					drawFrame(29, 9, 75, 37, Sonno1);
					expr++;
				} else if (expr == 1) {
					drawFrame(29,5,82,43,Sonno2);
					expr = 0;
				} 
				FaceAuxTime = millis();
			}




			checkProtoPieSerial("print");
			if (messaggio != "") {
        currentState = STATE_MUST_DELIVER;
				u8g2.sendF("c", 0xa7); //verde
				drawFrame(29,9,70,45,Normal1);
				expr = 0;
				FaceAuxTime = millis();
      }
      break;



    case STATE_MUST_DELIVER:
			normalFace();
			

      Distance = analogRead(PROXP);

      if (Distance > 220) {
        currentState = STATE_DELIVERY;
      }
      break;

  

    case STATE_DELIVERY:
			u8g2.sendF("c", 0xa6);//nero per lettera
			drawFrame(39,20,49,23,Lettera1);

			tone(PIEZP, 1047, 60);  
  		delay(80);
  		tone(PIEZP, 1568, 60);  
  		delay(80);
  		tone(PIEZP, 2093, 120); 
  		delay(150);
  		noTone(PIEZP);

			drawFrame(39,14,49,29,Lettera2);
			delay(800);
      write(messaggio, 1);
			Serial.println(F("force||"));
      delay(10000);

			messaggio = "";
      currentState = STATE_WAITING_FOR_FEEDBACK;
      u8g2.sendF("c", 0xa7); //verde
			drawFrame(29,9,70,45,Normal1);
			expr = 0;
			FaceAuxTime = millis();
			StateAuxTime = millis();
      break;



    case STATE_WAITING_FOR_FEEDBACK:
			normalFace();


      if (millis() - StateAuxTime > 30000) {
        Serial.println(F("action||null"));
				returnToIdle();
      }

			detectFeedback();
			if (feedbackType > 0) {
				currentState = STATE_WAITING_FOR_RESPONSE;
      	StateAuxTime = millis();
				drawFrame(29,9,70,39,Stordimento1);
				tone(PIEZP, 1568, 60);  
  			delay(80);
  			noTone(PIEZP);
				feedbackType = 0;
			}
      break;



    case STATE_WAITING_FOR_RESPONSE:

			if (millis() - StateAuxTime < 3000) {
				if (millis() - FaceAuxTime > 600) {
					if (expr == 0) {
						drawFrame(29,9,70,39,Stordimento1);
						expr++;
						FaceAuxTime = millis();
					} else if (expr == 1) {
						drawFrame(29,9,70,39,Stordimento2);
						expr = 0;
						FaceAuxTime = millis();
					}
				}
      } else if (millis() - StateAuxTime > 3000) {
				normalFace();
			}
			
			checkProtoPieSerial("mood");
			if (messaggio != "") {
    		feedbackType = messaggio.toInt();
				if (feedbackType == 1) {
					for (int i = 0; i < 12; i++) {
						drawFrame(29, 9, 70, 45, Amore1);
            delay(300);
            drawFrame(29, 9, 70, 45, Amore2);
            tone(PIEZP, 1318, 80);
            delay(100);
            tone(PIEZP, 1568, 100);
            delay(100);
					}
				} else if (feedbackType == 2) {
					for (int i = 0; i < 12; i++) {
						drawFrame(24,9,80,45,Felice1);
            delay(300);
            drawFrame(22,9,86,45,Felice2);
            tone(PIEZP, 1318, 80);
            delay(100);
            tone(PIEZP, 1568, 100);
            delay(100);
					}
				} else if (feedbackType == 3) {
					for (int i = 0; i < 6; i++) {
						drawFrame(29, 8, 70, 47, Triste1);
            delay(500);
            drawFrame(29, 8, 70, 49, Triste2);
            tone(PIEZP, 330, 200);
            delay(250);
            tone(PIEZP, 261, 400);
            delay(200);
					}
				} else if (feedbackType == 4) {
					for (int i = 0; i < 20; i++) {
						drawFrame(29, 5, 83, 47, Rabbia1);
            delay(200);
            drawFrame(29, 7, 78, 45, Rabbia2);
            tone(PIEZP, 110, 80);
            delay(50);
            tone(PIEZP, 123, 150);
            delay(50);
					}
				}
			returnToIdle();
			}

      if (millis() - StateAuxTime > 30000) {
        returnToIdle();
      }
      break;

  }
  delay(20); 
}



